<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5" />
    </startup>
    <system.serviceModel>
        <bindings>
            <wsDualHttpBinding>
                <binding name="WSDualHttpBinding_IDataCollectorService" maxReceivedMessageSize="524288" />
                <binding name="WSDualHttpBinding_IProcessDataService" maxReceivedMessageSize="524288" />
                <binding name="WSDualHttpBinding_IUserService" maxReceivedMessageSize="524288" />
            </wsDualHttpBinding>
        </bindings>
        <client>
            <endpoint address="http://localhost:9010/MonitoringService/DataCollectorService/wsDualHttp"
                binding="wsDualHttpBinding" bindingConfiguration="WSDualHttpBinding_IDataCollectorService"
                contract="DataCollectorServiceReference.IDataCollectorService"
                name="WSDualHttpBinding_IDataCollectorService">
                <identity>
                    <userPrincipalName value="ma.ramezani@SBO.COM" />
                </identity>
            </endpoint>
            <endpoint address="http://localhost:9010/MonitoringService/UserService/wsDualHttp"
                binding="wsDualHttpBinding" bindingConfiguration="WSDualHttpBinding_IUserService"
                contract="UsersServiceReference.IUserService" name="WSDualHttpBinding_IUserService">
                <identity>
                    <dns value="localhost" />
                </identity>
            </endpoint>
            <endpoint address="http://localhost:9010/MonitoringService/ProcessDataService/wsDualHttp"
                binding="wsDualHttpBinding" bindingConfiguration="WSDualHttpBinding_IProcessDataService"
                contract="ProcessDataServiceReference.IProcessDataService"
                name="WSDualHttpBinding_IProcessDataService">
                <identity>
                    <userPrincipalName value="ma.ramezani@SBO.COM" />
                </identity>
            </endpoint>
        </client>
    </system.serviceModel>
</configuration>