<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5" />
    </startup>
    <system.serviceModel>
        <bindings>
            <basicHttpBinding>
                <binding name="BasicHttpBinding_IDataCollectorService" />
            </basicHttpBinding>
            <wsDualHttpBinding>
                <binding name="WSDualHttpBinding_IDataCollectorService" />
            </wsDualHttpBinding>
        </bindings>
        <client>
            <endpoint address="http://localhost:19532/DataCollectorService.svc"
                binding="basicHttpBinding" bindingConfiguration="BasicHttpBinding_IDataCollectorService"
